    "ANIMATION_SCREEN_SECTION_1_DETAIL" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "In fact, animation is a sequence of pictures which shift quickly, then your brain blends them together let you feel they are moving. In professional terms, this is called persistence of vision."
          }
        },
        "zh-Hans" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "实际上，动画是一系列快速切换的图片，然后你的大脑将它们混合在一起，让你觉得它们在移动。在专业术语中，这被称为视觉暂留。"
          }
        }
      }
    },
    "ANIMATION_SCREEN_SECTION_1_TITLE" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "How we see animation?"
          }
        },
        "zh-Hans" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "我们是如何看见动画的？"
          }
        }
      }
    },
    "ANIMATION_SCREEN_SECTION_2_DETAIL_1" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Animation makes characters come alive in movies, games and other literary and artistic works."
